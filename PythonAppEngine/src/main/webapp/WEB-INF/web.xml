<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <servlet>
        <servlet-name>com.gmail.cjbooms.thesis.pythonappengine.PythonAppEngine FileSystemService</servlet-name>
        <servlet-class>com.gmail.cjbooms.thesis.pythonappengine.server.FileSystemServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.gmail.cjbooms.thesis.pythonappengine.PythonAppEngine GitCommandsService</servlet-name>
        <servlet-class>com.gmail.cjbooms.thesis.pythonappengine.server.git.GitCommandsServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.gmail.cjbooms.thesis.pythonappengine.PythonAppEngine AppEngineService</servlet-name>
        <servlet-class>com.gmail.cjbooms.thesis.pythonappengine.server.AppEngineServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.gmail.cjbooms.thesis.pythonappengine.PythonAppEngine FileSystemService</servlet-name>
        <url-pattern>/pythonappengine/fs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.gmail.cjbooms.thesis.pythonappengine.PythonAppEngine GitCommandsService</servlet-name>
        <url-pattern>/pythonappengine/gs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.gmail.cjbooms.thesis.pythonappengine.PythonAppEngine AppEngineService</servlet-name>
        <url-pattern>/pythonappengine/ae</url-pattern>
    </servlet-mapping>


    <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>PythonAppEngine.html</welcome-file>
  </welcome-file-list>

</web-app>
